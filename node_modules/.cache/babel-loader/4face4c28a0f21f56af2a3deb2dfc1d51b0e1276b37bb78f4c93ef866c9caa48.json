{"ast":null,"code":"import { getApps } from \"firebase/app\";\nimport { getAuth } from \"firebase/auth\";\nimport { getFirestore, collection, getDocs } from \"firebase/firestore\";\nimport axios from \"axios\";\nexport default {\n  name: \"HomePage\",\n  data() {\n    return {\n      firebaseCheckMsg: \"\",\n      currentShoeIndex: 0,\n      userName: \"\",\n      API_KEY: \"XhTXaEmjhKFZiHgCgD6bYUi8\",\n      REMOVE_BG_API_URL: \"https://api.remove.bg/v1.0/removebg\",\n      shoes: []\n    };\n  },\n  computed: {\n    randomShoes() {\n      return [...this.shoes].sort(() => 0.5 - Math.random()).slice(0, 5);\n    }\n  },\n  methods: {\n    async removeBackgroundFromShoe(imageURL) {\n      try {\n        const response = await axios.post(this.REMOVE_BG_API_URL, {\n          image_url: imageURL,\n          size: \"auto\"\n        }, {\n          headers: {\n            \"X-Api-Key\": this.API_KEY\n          }\n        });\n        return `data:image/png;base64,${response.data.result}`;\n      } catch (error) {\n        console.error(\"Error removing background:\", error);\n        throw error;\n      }\n    },\n    nextShoe() {\n      if (this.shoes.length) {\n        this.currentShoeIndex = (this.currentShoeIndex + 1) % this.shoes.length;\n      }\n    },\n    async fetchShoes() {\n      try {\n        const db = getFirestore();\n        const shoesCollection = collection(db, \"shoes\");\n        const shoeSnapshot = await getDocs(shoesCollection);\n        let shoesData = shoeSnapshot.docs.map(doc => doc.data());\n        this.shoes = shoesData;\n      } catch (error) {\n        console.error(\"Error fetching shoes:\", error);\n      }\n    },\n    checkFirebase() {\n      const apps = getApps();\n      if (apps.length) {\n        this.firebaseCheckMsg = \"Firebase er online!\";\n      } else {\n        this.firebaseCheckMsg = \"Firebase er offline!\";\n      }\n    }\n  },\n  created() {\n    this.fetchShoes();\n    const auth = getAuth();\n    if (auth.currentUser) {\n      this.userName = auth.currentUser.email || \"User\";\n    }\n    setInterval(this.nextShoe, 5000);\n  }\n};","map":{"version":3,"names":["getApps","getAuth","getFirestore","collection","getDocs","axios","name","data","firebaseCheckMsg","currentShoeIndex","userName","API_KEY","REMOVE_BG_API_URL","shoes","computed","randomShoes","sort","Math","random","slice","methods","removeBackgroundFromShoe","imageURL","response","post","image_url","size","headers","result","error","console","nextShoe","length","fetchShoes","db","shoesCollection","shoeSnapshot","shoesData","docs","map","doc","checkFirebase","apps","created","auth","currentUser","email","setInterval"],"sources":["src/views/HomePage.vue"],"sourcesContent":["<template>\r\n  <div>\r\n    <div class=\"frontpage-container\">\r\n      <div class=\"frontpage-parent\">\r\n        <h1>Velkommen til NIDIDAS</h1>\r\n        <h2>Frigør skoens ekspertise</h2>\r\n        <p>\r\n          Opdag NIDIDAS - hvor hvert skridt fortæller en historie. Fra de\r\n          tidløse klassikere til de mest dristige nyskabelser, vores sko er mere\r\n          end bare fodtøj. De er en livsstil, et udtryk og et vidnesbyrd om\r\n          upåklagelig design og håndværk. Dyk ned i vores kuraterede kollektion\r\n          og lad dine fødder nyde komforten, stilen og elegance, der er unikt\r\n          for NIDIDAS.\r\n        </p>\r\n        <p>Interesseret i at lære mere om os? Du er kun et skridt væk.</p>\r\n      </div>\r\n\r\n      <section class=\"showShoes\">\r\n        <h3>Nogle Af Vores Sko</h3>\r\n        <transition name=\"fade\" mode=\"out-in\">\r\n          <div class=\"single-shoe\" v-if=\"shoes.length\" :key=\"currentShoeIndex\">\r\n            <img\r\n              :src=\"shoes[currentShoeIndex].image\"\r\n              :alt=\"shoes[currentShoeIndex].name\"\r\n            />\r\n\r\n            <h3>{{ shoes[currentShoeIndex].name }}</h3>\r\n            <p>{{ shoes[currentShoeIndex].description }}</p>\r\n            <h4><strong>Pris:</strong> {{ shoes[currentShoeIndex].price }}</h4>\r\n            <button>Se Dette Produkt [VIRKER IKKE]</button>\r\n          </div>\r\n        </transition>\r\n      </section>\r\n    </div>\r\n  </div>\r\n</template>\r\n\r\n<style scoped>\r\n/* Define light theme colors */\r\n:root {\r\n  --primary-bg-color: #ffffff;\r\n  --secondary-bg-color: #f7f7f7;\r\n  --primary-text-color: #333333;\r\n  --secondary-text-color: #666666;\r\n  --btn-color: #e91e63;\r\n  --border-color: black;\r\n}\r\n\r\n/* Define dark theme colors */\r\n[data-theme=\"dark\"] {\r\n  --primary-bg-color: #333333;\r\n  --secondary-bg-color: #444444;\r\n  --primary-text-color: #ffffff;\r\n  --secondary-text-color: #bbbbbb;\r\n  --btn-color: #ff4081;\r\n  --border-color: white;\r\n}\r\n\r\n.frontpage-container {\r\n  padding: 2rem;\r\n  color: var(--primary-text-color);\r\n  display: flex;\r\n  justify-content: space-between;\r\n}\r\n\r\nh4 {\r\n  margin-top: 20px;\r\n}\r\n\r\n@keyframes fadeIn {\r\n  from {\r\n    opacity: 0;\r\n  }\r\n  to {\r\n    opacity: 1;\r\n  }\r\n}\r\n\r\n@keyframes slideInFromBottom {\r\n  0% {\r\n    transform: translateY(100%);\r\n  }\r\n  100% {\r\n    transform: translateY(0);\r\n  }\r\n}\r\n\r\n.showShoes {\r\n  text-align: center;\r\n  padding: 2rem;\r\n  width: 25%;\r\n  background-color: var(--secondary-bg-color);\r\n  border-radius: 10px;\r\n  display: flex;\r\n  justify-content: center;\r\n  align-items: center;\r\n  flex-direction: column;\r\n}\r\n\r\n.showShoes h3 {\r\n  font-size: 1.5rem;\r\n  margin-bottom: 5%;\r\n}\r\n\r\n.fade-enter-active,\r\n.fade-leave-active {\r\n  transition: opacity 1s;\r\n}\r\n.fade-enter,\r\n.fade-leave-to {\r\n  opacity: 0;\r\n}\r\n\r\n.single-shoe {\r\n  width: 80%;\r\n  height: 100%;\r\n  border-radius: 5px;\r\n  padding: 1rem;\r\n  border: 2px solid var(--primary-text-color);\r\n  font-size: 1.5rem;\r\n}\r\n\r\n.single-shoe p {\r\n  margin-top: 1rem;\r\n  font-size: 1rem;\r\n}\r\n\r\n.single-shoe img {\r\n  width: 100%;\r\n  height: 300px;\r\n  object-fit: cover;\r\n  border-radius: 10px;\r\n}\r\n\r\n/* Intro Side */\r\n.frontpage-parent {\r\n  width: 60%;\r\n  text-align: center;\r\n  display: flex;\r\n  justify-content: center;\r\n  align-items: center;\r\n  flex-direction: column;\r\n}\r\n\r\n.frontpage-parent h1 {\r\n  color: var(--primary-text-color);\r\n  font-size: 3rem;\r\n  margin-bottom: 1rem;\r\n}\r\n\r\n.frontpage-parent h2 {\r\n  color: var(--primary-text-color);\r\n  font-size: 2rem;\r\n  margin-bottom: 1rem;\r\n}\r\n\r\n.frontpage-parent p {\r\n  color: var(--primary-text-color);\r\n  font-size: 1rem;\r\n  margin-bottom: 1rem;\r\n}\r\n\r\n@media (max-width: 768px) {\r\n  .frontpage-parent {\r\n    width: 100%;\r\n  }\r\n  .frontpage-container {\r\n    flex-direction: column;\r\n  }\r\n  .showShoes {\r\n    width: 100%;\r\n  }\r\n  .single-shoe {\r\n    width: 100%;\r\n  }\r\n}\r\n</style>\r\n\r\n<script>\r\nimport { getApps } from \"firebase/app\";\r\nimport { getAuth } from \"firebase/auth\";\r\nimport { getFirestore, collection, getDocs } from \"firebase/firestore\";\r\nimport axios from \"axios\";\r\n\r\nexport default {\r\n  name: \"HomePage\",\r\n  data() {\r\n    return {\r\n      firebaseCheckMsg: \"\",\r\n      currentShoeIndex: 0,\r\n      userName: \"\",\r\n      API_KEY: \"XhTXaEmjhKFZiHgCgD6bYUi8\",\r\n      REMOVE_BG_API_URL: \"https://api.remove.bg/v1.0/removebg\",\r\n      shoes: [],\r\n    };\r\n  },\r\n  computed: {\r\n    randomShoes() {\r\n      return [...this.shoes].sort(() => 0.5 - Math.random()).slice(0, 5);\r\n    },\r\n  },\r\n  methods: {\r\n    async removeBackgroundFromShoe(imageURL) {\r\n      try {\r\n        const response = await axios.post(\r\n          this.REMOVE_BG_API_URL,\r\n          {\r\n            image_url: imageURL,\r\n            size: \"auto\",\r\n          },\r\n          {\r\n            headers: {\r\n              \"X-Api-Key\": this.API_KEY,\r\n            },\r\n          }\r\n        );\r\n        return `data:image/png;base64,${response.data.result}`;\r\n      } catch (error) {\r\n        console.error(\"Error removing background:\", error);\r\n        throw error;\r\n      }\r\n    },\r\n    nextShoe() {\r\n      if (this.shoes.length) {\r\n        this.currentShoeIndex = (this.currentShoeIndex + 1) % this.shoes.length;\r\n      }\r\n    },\r\n    async fetchShoes() {\r\n      try {\r\n        const db = getFirestore();\r\n        const shoesCollection = collection(db, \"shoes\");\r\n        const shoeSnapshot = await getDocs(shoesCollection);\r\n        let shoesData = shoeSnapshot.docs.map((doc) => doc.data());\r\n\r\n        this.shoes = shoesData;\r\n      } catch (error) {\r\n        console.error(\"Error fetching shoes:\", error);\r\n      }\r\n    },\r\n\r\n    checkFirebase() {\r\n      const apps = getApps();\r\n      if (apps.length) {\r\n        this.firebaseCheckMsg = \"Firebase er online!\";\r\n      } else {\r\n        this.firebaseCheckMsg = \"Firebase er offline!\";\r\n      }\r\n    },\r\n  },\r\n  created() {\r\n    this.fetchShoes();\r\n    const auth = getAuth();\r\n    if (auth.currentUser) {\r\n      this.userName = auth.currentUser.email || \"User\";\r\n    }\r\n    setInterval(this.nextShoe, 5000);\r\n  },\r\n};\r\n</script>\r\n"],"mappings":"AAmLA,SAAAA,OAAA;AACA,SAAAC,OAAA;AACA,SAAAC,YAAA,EAAAC,UAAA,EAAAC,OAAA;AACA,OAAAC,KAAA;AAEA;EACAC,IAAA;EACAC,KAAA;IACA;MACAC,gBAAA;MACAC,gBAAA;MACAC,QAAA;MACAC,OAAA;MACAC,iBAAA;MACAC,KAAA;IACA;EACA;EACAC,QAAA;IACAC,YAAA;MACA,gBAAAF,KAAA,EAAAG,IAAA,aAAAC,IAAA,CAAAC,MAAA,IAAAC,KAAA;IACA;EACA;EACAC,OAAA;IACA,MAAAC,yBAAAC,QAAA;MACA;QACA,MAAAC,QAAA,SAAAlB,KAAA,CAAAmB,IAAA,CACA,KAAAZ,iBAAA,EACA;UACAa,SAAA,EAAAH,QAAA;UACAI,IAAA;QACA,GACA;UACAC,OAAA;YACA,kBAAAhB;UACA;QACA,CACA;QACA,gCAAAY,QAAA,CAAAhB,IAAA,CAAAqB,MAAA;MACA,SAAAC,KAAA;QACAC,OAAA,CAAAD,KAAA,+BAAAA,KAAA;QACA,MAAAA,KAAA;MACA;IACA;IACAE,SAAA;MACA,SAAAlB,KAAA,CAAAmB,MAAA;QACA,KAAAvB,gBAAA,SAAAA,gBAAA,aAAAI,KAAA,CAAAmB,MAAA;MACA;IACA;IACA,MAAAC,WAAA;MACA;QACA,MA