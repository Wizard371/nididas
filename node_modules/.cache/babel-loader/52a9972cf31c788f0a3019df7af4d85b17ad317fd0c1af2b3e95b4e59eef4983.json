{"ast":null,"code":"import \"core-js/modules/es.array.push.js\";\nimport { getAuth, onAuthStateChanged, signOut } from \"firebase/auth\";\nimport { getFirestore, doc, getDoc } from \"firebase/firestore\";\nimport router from \"@/router\";\nimport AddShoeModal from \"@/components/AddShoeModal.vue\";\nexport default {\n  components: {\n    AddShoeModal\n  },\n  data() {\n    return {\n      isModalOpen: false,\n      user: null,\n      role: null\n    };\n  },\n  created() {\n    const auth = getAuth();\n    const db = getFirestore();\n    onAuthStateChanged(auth, async currentUser => {\n      if (currentUser) {\n        this.user = currentUser;\n\n        // Fetch role from Firestore\n        const userDocRef = doc(db, \"users\", currentUser.uid);\n        const userDocSnap = await getDoc(userDocRef);\n        if (userDocSnap.exists()) {\n          this.role = userDocSnap.data().role;\n        }\n      } else {\n        if (router.currentRoute.path !== \"/login\") {\n          router.push(\"/login\");\n        }\n      }\n    });\n  },\n  methods: {\n    async handleLogout() {\n      try {\n        const auth = getAuth();\n        await signOut(auth);\n        this.user = null;\n        this.$toast.success(\"Loggede ud.\");\n      } catch (error) {\n        console.error(\"Kunne ikke logge ud:\", error);\n      }\n    },\n    openModal() {\n      this.isModalOpen = true;\n    },\n    closeModal() {\n      this.isModalOpen = false;\n    },\n    shoeAdded() {\n      this.isModalOpen = false;\n    }\n  }\n};","map":{"version":3,"names":["getAuth","onAuthStateChanged","signOut","getFirestore","doc","getDoc","router","AddShoeModal","components","data","isModalOpen","user","role","created","auth","db","currentUser","userDocRef","uid","userDocSnap","exists","currentRoute","path","push","methods","handleLogout","$toast","success","error","console","openModal","closeModal","shoeAdded"],"sources":["src/views/KontoPage.vue"],"sourcesContent":["<template>\r\n  <div>\r\n    <h1>KONTO SIDE</h1>\r\n    <div v-if=\"user\">\r\n      <p>Your email: {{ user.email }}</p>\r\n      <button @click=\"handleLogout\">Log Ud</button>\r\n      <button @click=\"openModal\" class=\"add-btn\">Tilf√∏j Sko</button>\r\n      <AddShoeModal\r\n        :isOpen=\"isModalOpen\"\r\n        @close=\"closeModal\"\r\n        @added=\"shoeAdded\"\r\n      ></AddShoeModal>\r\n      <button v-if=\"role === 'admin'\" @click=\"openAdminPanel\">\r\n        Admin Panel\r\n      </button>\r\n    </div>\r\n  </div>\r\n</template>\r\n\r\n<script>\r\nimport { getAuth, onAuthStateChanged, signOut } from \"firebase/auth\";\r\nimport { getFirestore, doc, getDoc } from \"firebase/firestore\";\r\nimport router from \"@/router\";\r\nimport AddShoeModal from \"@/components/AddShoeModal.vue\";\r\n\r\nexport default {\r\n  components: {\r\n    AddShoeModal,\r\n  },\r\n  data() {\r\n    return {\r\n      isModalOpen: false,\r\n      user: null,\r\n      role: null,\r\n    };\r\n  },\r\n  created() {\r\n    const auth = getAuth();\r\n    const db = getFirestore();\r\n\r\n    onAuthStateChanged(auth, async (currentUser) => {\r\n      if (currentUser) {\r\n        this.user = currentUser;\r\n\r\n        // Fetch role from Firestore\r\n        const userDocRef = doc(db, \"users\", currentUser.uid);\r\n        const userDocSnap = await getDoc(userDocRef);\r\n        if (userDocSnap.exists()) {\r\n          this.role = userDocSnap.data().role;\r\n        }\r\n      } else {\r\n        if (router.currentRoute.path !== \"/login\") {\r\n          router.push(\"/login\");\r\n        }\r\n      }\r\n    });\r\n  },\r\n  methods: {\r\n    async handleLogout() {\r\n      try {\r\n        const auth = getAuth();\r\n        await signOut(auth);\r\n        this.user = null;\r\n        this.$toast.success(\"Loggede ud.\");\r\n      } catch (error) {\r\n        console.error(\"Kunne ikke logge ud:\", error);\r\n      }\r\n    },\r\n    openModal() {\r\n      this.isModalOpen = true;\r\n    },\r\n    closeModal() {\r\n      this.isModalOpen = false;\r\n    },\r\n    shoeAdded() {\r\n      this.isModalOpen = false;\r\n    },\r\n  },\r\n};\r\n</script>\r\n"],"mappings":";AAoBA,SAAAA,OAAA,EAAAC,kBAAA,EAAAC,OAAA;AACA,SAAAC,YAAA,EAAAC,GAAA,EAAAC,MAAA;AACA,OAAAC,MAAA;AACA,OAAAC,YAAA;AAEA;EACAC,UAAA;IACAD;EACA;EACAE,KAAA;IACA;MACAC,WAAA;MACAC,IAAA;MACAC,IAAA;IACA;EACA;EACAC,QAAA;IACA,MAAAC,IAAA,GAAAd,OAAA;IACA,MAAAe,EAAA,GAAAZ,YAAA;IAEAF,kBAAA,CAAAa,IAAA,QAAAE,WAAA;MACA,IAAAA,WAAA;QACA,KAAAL,IAAA,GAAAK,WAAA;;QAEA;QACA,MAAAC,UAAA,GAAAb,GAAA,CAAAW,EAAA,WAAAC,WAAA,CAAAE,GAAA;QACA,MAAAC,WAAA,SAAAd,MAAA,CAAAY,UAAA;QACA,IAAAE,WAAA,CAAAC,MAAA;UACA,KAAAR,IAAA,GAAAO,WAAA,CAAAV,IAAA,GAAAG,IAAA;QACA;MACA;QACA,IAAAN,MAAA,CAAAe,YAAA,CAAAC,IAAA;UACAhB,MAAA,CAAAiB,IAAA;QACA;MACA;IACA;EACA;EACAC,OAAA;IACA,MAAAC,aAAA;MACA;QACA,MAAAX,IAAA,GAAAd,OAAA;QACA,MAAAE,OAAA,CAAAY,IAAA;QACA,KAAAH,IAAA;QACA,KAAAe,MAAA,CAAAC,OAAA;MACA,SAAAC,KAAA;QACAC,OAAA,CAAAD,KAAA,yBAAAA,KAAA;MACA;IACA;IACAE,UAAA;MACA,KAAApB,WAAA;IACA;IACAqB,WAAA;MACA,KAAArB,WAAA;IACA;IACAsB,UAAA;MACA,KAAAtB,WAAA;IACA;EACA;AACA"},"metadata":{},"sourceType":"module","externalDependencies":[]}