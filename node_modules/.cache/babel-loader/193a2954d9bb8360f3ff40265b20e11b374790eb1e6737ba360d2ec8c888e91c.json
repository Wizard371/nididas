{"ast":null,"code":"import { ref } from \"vue\";\nimport { addDoc, collection } from \"firebase/firestore\";\nimport { db } from \"@/firebase\";\nexport default {\n  props: {\n    isOpen: Boolean\n  },\n  setup(props, {\n    emit\n  }) {\n    const shoe = ref({\n      name: \"\",\n      description: \"\",\n      image: \"\",\n      price: 0\n    });\n    async function addShoe() {\n      try {\n        const docRef = await addDoc(collection(db, \"shoes\"), shoe.value);\n        console.log(\"Document written with ID: \", docRef.id);\n        emit(\"added\");\n        close();\n      } catch (e) {\n        console.error(\"Error adding document: \", e);\n      }\n    }\n    function close() {\n      emit(\"close\");\n    }\n    return {\n      shoe,\n      addShoe,\n      close\n    };\n  }\n};","map":{"version":3,"names":["ref","addDoc","collection","db","props","isOpen","Boolean","setup","emit","shoe","name","description","image","price","addShoe","docRef","value","console","log","id","close","e","error"],"sources":["src/components/AddShoeModal.vue"],"sourcesContent":["<template>\r\n  <div v-if=\"isOpen\" class=\"modal\">\r\n    <div class=\"modal-content\">\r\n      <h3>Add a new shoe</h3>\r\n      <label>Name:</label>\r\n      <input v-model=\"shoe.name\" type=\"text\" />\r\n\r\n      <label>Description:</label>\r\n      <textarea v-model=\"shoe.description\"></textarea>\r\n\r\n      <label>Image URL:</label>\r\n      <input v-model=\"shoe.image\" type=\"text\" />\r\n\r\n      <label>Price ($):</label>\r\n      <input\r\n        v-model.number=\"shoe.price\"\r\n        type=\"number\"\r\n        step=\"0.01\"\r\n        placeholder=\"Enter price\"\r\n      />\r\n\r\n      <button @click=\"addShoe\">Add Shoe</button>\r\n      <button @click=\"close\">Cancel</button>\r\n    </div>\r\n  </div>\r\n</template>\r\n\r\n<script>\r\nimport { ref } from \"vue\";\r\nimport { addDoc, collection } from \"firebase/firestore\";\r\nimport { db } from \"@/firebase\";\r\n\r\nexport default {\r\n  props: {\r\n    isOpen: Boolean,\r\n  },\r\n  setup(props, { emit }) {\r\n    const shoe = ref({ name: \"\", description: \"\", image: \"\", price: 0 });\r\n\r\n    async function addShoe() {\r\n      try {\r\n        const docRef = await addDoc(collection(db, \"shoes\"), shoe.value);\r\n        console.log(\"Document written with ID: \", docRef.id);\r\n        emit(\"added\");\r\n        close();\r\n      } catch (e) {\r\n        console.error(\"Error adding document: \", e);\r\n      }\r\n    }\r\n\r\n    function close() {\r\n      emit(\"close\");\r\n    }\r\n\r\n    return {\r\n      shoe,\r\n      addShoe,\r\n      close,\r\n    };\r\n  },\r\n};\r\n</script>\r\n\r\n<style scoped>\r\n.modal {\r\n  position: fixed;\r\n  top: 0;\r\n  left: 0;\r\n  width: 100%;\r\n  height: 100%;\r\n  background-color: rgba(0, 0, 0, 0.6);\r\n  display: flex;\r\n  align-items: center;\r\n  justify-content: center;\r\n}\r\n\r\nlabel,\r\nbutton {\r\n  margin: 10px;\r\n  color: black;\r\n}\r\n\r\ninput {\r\n  background-color: white;\r\n  border: 1px solid black;\r\n  padding: 5px 10px;\r\n  color: black;\r\n}\r\n\r\n.modal-content {\r\n  display: flex;\r\n  flex-direction: column;\r\n  background-color: #fff;\r\n  padding: 20px;\r\n  border-radius: 5px;\r\n  width: 50%;\r\n  max-width: 100%;\r\n  margin: 10px;\r\n}\r\n</style>\r\n"],"mappings":"AA4BA,SAAAA,GAAA;AACA,SAAAC,MAAA,EAAAC,UAAA;AACA,SAAAC,EAAA;AAEA;EACAC,KAAA;IACAC,MAAA,EAAAC;EACA;EACAC,MAAAH,KAAA;IAAAI;EAAA;IACA,MAAAC,IAAA,GAAAT,GAAA;MAAAU,IAAA;MAAAC,WAAA;MAAAC,KAAA;MAAAC,KAAA;IAAA;IAEA,eAAAC,QAAA;MACA;QACA,MAAAC,MAAA,SAAAd,MAAA,CAAAC,UAAA,CAAAC,EAAA,YAAAM,IAAA,CAAAO,KAAA;QACAC,OAAA,CAAAC,GAAA,+BAAAH,MAAA,CAAAI,EAAA;QACAX,IAAA;QACAY,KAAA;MACA,SAAAC,CAAA;QACAJ,OAAA,CAAAK,KAAA,4BAAAD,CAAA;MACA;IACA;IAEA,SAAAD,MAAA;MACAZ,IAAA;IACA;IAEA;MACAC,IAAA;MACAK,OAAA;MACAM;IACA;EACA;AACA"},"metadata":{},"sourceType":"module","externalDependencies":[]}